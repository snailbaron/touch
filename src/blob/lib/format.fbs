namespace blob.fb;

struct Frame {
  x:uint32;
  y:uint32;
  w:uint32;
  h:uint32;
  ms:uint32;
}

table Blob {
  sheet:[uint8];

  names:string;
  name_offsets:[uint32];

  frames:[Frame];
  frame_offsets:[uint32];

  character_count:uint32;
  texture_count:uint32;
  object_count:uint32;
}

root_type Blob;
